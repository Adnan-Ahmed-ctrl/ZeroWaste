        <section class="py-5 text-center container">
          <div class="row py-lg-5">
            <div class="col-lg-6 col-md-8 mx-auto">
              <h1 class="fw-light">H.O.P</h1>
              <p class="lead text-muted"></p>
            </div>
          </div>
        </section>
      <main>
        <div class="album py-5 bg-light">
          <div class="container col-md-12" >
            <div class="my-3 p-3 bg-body rounded shadow-sm">
              <table>
                <tr>
              <td><span class="border-bottom pb-2 mb-0 mx-5">Name</span></td>
              <td><span class="border-bottom pb-2 mb-0 mx-5 ">Phone</span></td>
              <td><span class="border-bottom pb-2 mb-0 mx-5">Kebele</span></td>
              <td><span class="border-bottom pb-2 mb-0 mx-5">person</span></td>
              <td><span class="border-bottom pb-2 mb-0 mx-5 ">Account</span></td>
              <td><span class="border-bottom pb-2 mb-0 mx-5 px-4">Type</span></td>
              <td><span class="border-bottom pb-2 mb-0 mx-3">Approve</span></td>
              <td><span class="border-bottom pb-2 mb-0 mx-3">Disapprove</span></td>
            </tr>
              <div class="d-flex text-muted pt-3">
              </table>
                <table>
                <form action="/HOP_AD" method="post">
                  <% for (var i=0; i < counter; i++) { %>
                    
                <div class="pb-3 ">
                  <div class="d-flex justify-content-between">
                    
                      <tr>
                        <td><strong class="text-gray-dark"><%= name[i]%></strong></td>
                        <td><span class="d-block mx-5"><%= number[i]%></span></td>
                        <td><span class="d-block mx-5"><%= address[i]%></span></td>
                        <td><span class="d-block mx-5 px-4"><%= person[i]%> <span></td>
                        <td><span class="d-block mx-5"><%= account[i]%></span></td>
                        <td><span class="d-block mx-5"><%= type[i]%></span></td>
                    <td>    
                  <div class="d-flex justify-content-between align-items-center ">
                    <small class="text-muted mx-5"> <input type="checkbox" name="checker" id="" value="approved">
                      </small>
                    <small class="text-muted "> <input type="checkbox" name="checker" id="" value="disapproved">
                      </small>
                    </td>
                  </div>
                </td>
                </tr>
                  </div>
                </div>
                <% } %>
              </table>
              </div>
              <button type="submit" class="btn btn-primary col-lg-12" name="next" value="next">Submit</button>
              <small class="d-block text-end mt-3">
                
              </small>
            </div>
          </form>
          </div>
          
          </main>
          <div class="bg-white  py-3">
            <footer class="py-3 mb-2">
